package default_AccessModifiers_Example;

class Student {
	String name;
	int age;
	int rollno;
	
	void display() {
		System.out.println(name);
		System.out.println(age);
		System.out.println(rollno);
	}
}
   public class Main{
	   public static void main(String[]args ) {
		   Student student1  = new Student();
		   student1.name ="Reena";
		   student1.age =20;
		   student1.rollno =1001;  
	   }
   }