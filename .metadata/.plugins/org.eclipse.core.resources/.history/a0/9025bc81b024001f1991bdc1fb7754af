package Java_bean;

public class Main {
    public static void main(String[] args) {
        Address address1 = new Address(123);
        Student student1 = new Student("Alice", 20, address1);

        Address address2 = new Address(456);
        Student student2 = new Student("Bob", 22, address2);

        System.out.println("Student 1: Name - " + student1.getName() + ", Age - " + student1.getAge() + ", Address - " + student1.getAddress().getAddress());
        System.out.println("Student 2: Name - " + student2.getName() + ", Age - " + student2.getAge() + ", Address - " + student2.getAddress().getAddress());
    }
}