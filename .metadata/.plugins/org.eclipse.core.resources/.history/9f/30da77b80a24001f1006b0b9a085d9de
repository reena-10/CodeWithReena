public class Employee {
	
	int empolyeeid;
	String empolyeeName;
	String empolyeedepartment;
	int salary;
	String shift;
	 
	public String display() {
		return shift;
		
	}
	//default constructor
	Employee(){};
		
	//paramterized constructor
	public Employee(int empolyeeid , String empolyeeName, String empolyeedepartment , String shift, int salary) 
	{
		
		this.empolyeeName = empolyeeName;
		this.empolyeeid = empolyeeid;
		this.empolyeedepartment =empolyeedepartment;
		this.shift = shift;
		this.salary =salary;
	}
	
	

}